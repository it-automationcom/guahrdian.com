  <button type="button" onclick="disable()">Disable Map </button>
  <button type="button" onclick="enable()">Enable Map </button>
  <button type="button" onclick="disable_layers()">Disable Maplayers</button>
  <button type="button" onclick="enable_layers()">Enable Maplayers</button>
